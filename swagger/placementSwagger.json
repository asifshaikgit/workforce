                },
                "work_location_address_line_one": {
                  "example": "@_string/work_location_type_2",
                  "type": "string"
                },
                "work_location_address_line_two": {
                  "example": "string/work_location_type_2",
                  "type": "string"
                },
                "work_location_city": {
