                      "document_name": {
                        "type": "string",
                        "example": "string/Not_Mandatory"
                      },
                      "document_type_id": {
                        "type": "int",
                        "example": "Placement_Document_type_id"
                      },
                      "description": {
                        "type": "string",
                        "example": "Not_Mandatory"
                      },
                      "new_document_id": {
                        "type": "string",
                        "example": "Id_from_upload_document"
