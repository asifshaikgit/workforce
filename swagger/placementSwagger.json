                  "type": "integer"
                },
                "work_location_country_id": {
                  "example": "@_id_from_countries/work_location_type_2",
                  "type": "integer"
                },
                "work_location_zipcode": {
                  "example": "@_zip_code/work_location_type_2",
                  "type": "integer"
                },
                "documents": {
                  "type": "array",
                  "items": {
                    "type": "object",
