                  "example": "@_Only_Work_Email_Id"
                },
                "placed_employee_id": {
                  "example": "Employee_Id",
                  "type": "UUID"
                },
                "notice_period": {
                  "example": "@_Integer",
                  "type": "integer"
                },
                "start_date": {
                  "type": "date",
                  "example": "@_Date"
                },
                "end_date": {
                  "type": "date",
                  "example": "@_Date"
                },
                "work_location_type": {
                  "example": "@_Integer_1_or_2/1 for remote and 2 for on site i.e. client location",
